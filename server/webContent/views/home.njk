{% extends "layout.njk" %}
{% block right %}
<table class="table table-hover">
    <thead>
        <tr>
            <td>#</td><td>用户名</td><td>密码</td><td>创建时间</td><td>操作</td>
        </tr>
    </thead>
    <tbody>
{% for item in data.dataList %}
    <tr>
        <td>{{ item.id }}</td><td>{{ item.username }}</td><td>{{ item.password }}</td><td>{{ item.createTime }}</td><td><button type="button" class="btn btn-danger d-btn">禁用</button></td>
    </tr>
{% endfor %}
    </tbody>
</table>
{% endblock %}
